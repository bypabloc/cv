{
  "name": "minimalist-portfolio-json",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --remote",
    "db:seed": "astro db execute \"./db/set-data.ts\" --remote",
    "build": "astro check && astro build",
    "build:no-check": "astro build --remote",
    "preview": "astro preview",
    "astro": "astro",
    "start:dev": "ENV=development pnpm run start",
    "start": "node scripts/index.js",
    "npm:install": "rm -rf node_modules && rm -rf dist && pnpm store prune && pnpm install",
    "npm:reinstall": "rm -rf node_modules && rm -rf dist && rm -rf .nuxt && rm -f pnpm-lock.yaml && pnpm store prune && pnpm install"
  },
  "dependencies": {
    "@astrojs/check": "0.7.0",
    "@astrojs/db": "^0.11.6",
    "@astrojs/node": "^8.3.1",
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/vue": "^4.5.0",
    "@iconify-json/ant-design": "^1.1.16",
    "@iconify-json/carbon": "^1.1.36",
    "@iconify-json/clarity": "^1.1.13",
    "@iconify-json/material-symbols": "^1.1.82",
    "@iconify-json/mdi": "^1.1.67",
    "@nanostores/vue": "^0.10.0",
    "@unocss/astro": "^0.61.0",
    "astro": "^4.11.0",
    "dotenv": "^16.4.5",
    "hotkeypad": "^0.1.1",
    "nanostores": "^0.10.3",
    "typescript": "^5.5.2",
    "unocss": "^0.61.0",
    "uuid": "^10.0.0",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "archiver": "^7.0.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.2",
    "semver": "^7.6.2",
    "undici": "^6.19.0"
  }
}
