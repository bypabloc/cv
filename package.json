{
  "name": "minimalist-portfolio-json",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --remote",
    "db:seed": "astro db execute \"./db/set-data.ts\" --remote",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:no-check": "astro build --remote",
    "preview": "astro preview",
    "astro": "astro",
    "npm:install": "rm -rf node_modules && rm -rf dist && pnpm store prune && pnpm install",
    "npm:reinstall": "rm -rf node_modules && rm -rf dist && rm -rf .nuxt && rm -f pnpm-lock.yaml && pnpm store prune && pnpm install"
  },
  "dependencies": {
    "@astrojs/check": "0.7.0",
    "@astrojs/db": "^0.11.4",
    "@astrojs/node": "^8.2.5",
    "@astrojs/vue": "^4.3.0",
    "@iconify-json/ant-design": "^1.1.16",
    "@iconify-json/clarity": "^1.1.12",
    "@iconify-json/material-symbols": "^1.1.80",
    "@iconify-json/mdi": "^1.1.66",
    "@unocss/astro": "^0.60.3",
    "astro": "4.9.1",
    "dotenv": "^16.4.5",
    "hotkeypad": "0.1.1",
    "typescript": "5.4.5",
    "unocss": "^0.60.3",
    "uuid": "^9.0.1",
    "vue": "^3.4.27"
  }
}
